      <TextField fx:id="subTotal_TextField" editable="false" layoutX="8.0" layoutY="14.0" />
      <TextField fx:id="salesTax_TextField" editable="false" layoutX="8.0" layoutY="65.0" />
      <TextField fx:id="total_TextField" editable="false" layoutX="8.0" layoutY="116.0" />
